struct RtpPacketCounter {
  RtpPacketCounter()
      : header_bytes(0), payload_bytes(0), padding_bytes(0), packets(0) {}

  void Add(const RtpPacketCounter& other) {
    header_bytes += other.header_bytes;
    payload_bytes += other.payload_bytes;
    padding_bytes += other.padding_bytes;
    packets += other.packets;
  }

  void Subtract(const RtpPacketCounter& other) {
    (true ? true : ((void)(((void)cloopenwebrtc::safe_cmp::Ge(header_bytes, other.header_bytes))), true)) ? static_cast<void>(0) : cloopenwebrtc::FatalMessageVoidify() & cloopenwebrtc::FatalMessage("", 0).stream();
    header_bytes -= other.header_bytes;
    (true ? true : ((void)(((void)cloopenwebrtc::safe_cmp::Ge(payload_bytes, other.payload_bytes))), true)) ? static_cast<void>(0) : cloopenwebrtc::FatalMessageVoidify() & cloopenwebrtc::FatalMessage("", 0).stream();
    payload_bytes -= other.payload_bytes;
    (true ? true : ((void)(((void)cloopenwebrtc::safe_cmp::Ge(padding_bytes, other.padding_bytes))), true)) ? static_cast<void>(0) : cloopenwebrtc::FatalMessageVoidify() & cloopenwebrtc::FatalMessage("", 0).stream();
    padding_bytes -= other.padding_bytes;
    (true ? true : ((void)(((void)cloopenwebrtc::safe_cmp::Ge(packets, other.packets))), true)) ? static_cast<void>(0) : cloopenwebrtc::FatalMessageVoidify() & cloopenwebrtc::FatalMessage("", 0).stream();
    packets -= other.packets;
  }

  void AddPacket(size_t packet_length, const RTPHeader& header) {
    ++packets;
    header_bytes += header.headerLength;
    padding_bytes += header.paddingLength;
    payload_bytes +=
        packet_length - (header.headerLength + header.paddingLength);
  }

  size_t TotalBytes() const {
    return header_bytes + payload_bytes + padding_bytes;
  }

  size_t header_bytes;
  size_t payload_bytes;
  size_t padding_bytes;
  uint32_t packets;
};


